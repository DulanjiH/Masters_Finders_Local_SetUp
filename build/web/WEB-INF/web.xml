<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SignIn</servlet-name>
        <servlet-class>Servlets.SignIn</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>Servlets.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveCourceDetails</servlet-name>
        <servlet-class>Servlets.SaveCourceDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCourceDetails</servlet-name>
        <servlet-class>Servlets.SearchCourceDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDetails</servlet-name>
        <servlet-class>Servlets.LoadDetails</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignIn</servlet-name>
        <url-pattern>/SignIn</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveCourceDetails</servlet-name>
        <url-pattern>/SaveCourceDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCourceDetails</servlet-name>
        <url-pattern>/SearchCourceDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDetails</servlet-name>
        <url-pattern>/LoadDetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
